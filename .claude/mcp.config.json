{
  "mcpServers": {
    "claude-context": {
      "command": "npx",
      "args": [
        "-y",
        "@zilliz/claude-context-mcp@latest"
      ],
      "env": {        
      }
    },
    "context7-mcp": {
      "command": "npx",
      "args": [
    "-y",
    "@upstash/context7-mcp",
    "--api-key",
    "${SMITHERY_KEY}"
      ]
    },
    "mcp": {
      "command": "npx",
      "args": [
    "-y",
    "@smithery/cli@latest",
    "run",
    "@docfork/mcp",
    "--key",
    "${SMITHERY_KEY}"
      ]
    },
    "server-sequential-thinking": {
      "command": "npx",
      "args": [
    "-y",
    "@smithery/cli@latest",
    "run",
    "@smithery-ai/server-sequential-thinking",
    "--key",
    "${SMITHERY_KEY}",
    "--profile",
    "joint-constrictor-SZYWlg"
      ]
    }
  }
}